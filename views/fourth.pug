block content
        .content
            section.slideshow-container
                .mySlides(class="slides")
                    img.object-fit-cover(src="https://66.media.tumblr.com/e93599227e72f4b0a84bb28379411581/f1c086a0fc4808ce-ab/s1280x1920/388685685a1e3b5b79fd87f03bdb6a221b8d6d61.jpg")
